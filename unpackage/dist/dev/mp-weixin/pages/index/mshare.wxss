.zl-box {
  padding: 30rpx;
}
.zl-box .box-one {
  font-size: 26rpx;
}
.zl-box .box-one .text {
  margin: 10rpx 0;
}
.zl-box .box-one .weight {
  margin-top: 10rpx;
}
.zl-box .box-two {
  margin-top: 40rpx;
}
.zl-box .box-two .photo-box {
  position: relative;
}
.zl-box .box-two .photo-box .photo {
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  display: flex;
}
.zl-box .box-two .photo-box .photo image {
  width: 220rpx;
  height: 220rpx;
  border-radius: 10rpx;
  background-color: #F1F1F1;
}
.zl-box .box-two .photo-box .checkbox {
  position: absolute;
  right: -24rpx;
  top: 0;
}
.bottom-box {
  width: 100%;
  height: 180rpx;
  padding: 0 50rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 998;
}
.bottom-box .min-box {
  text-align: center;
  font-size: 22rpx;
  color: #666666;
}
.bottom-box .min-box image {
  width: 80rpx;
  height: 80rpx;
}
.popup-box {
  width: 100%;
}
.popup-box .canvas-box {
  background-color: #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: center;
}
.popup-box .save {
  width: 100%;
  margin: 20rpx auto;
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #2d407a;
  border-radius: 40rpx;
  font-size: 24rpx;
  text-align: center;
  color: #fff;
}

