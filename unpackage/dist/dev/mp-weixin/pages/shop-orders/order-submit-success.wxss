.prices {
  color: #ea3a4a;
  text-align: center;
  margin-top: 40rpx;
}
.prices .rmb {
  font-size: 32rpx;
}
.prices .integer {
  font-size: 48rpx;
  font-weight: bold;
}
.prices .fractional-part {
  font-size: 32rpx;
}
.submit-success-box-one {
  width: 100%;
  height: 230rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #F6F6F6;
}
.submit-success-box-one .one-title {
  text-align: center;
  font-size: 30rpx;
}
.submit-success-box-one .orders-number {
  font-size: 30rpx;
  color: #999999;
  margin-top: 30rpx;
  position: relative;
}
.submit-success-box-one .orders-number .copy {
  width: 70rpx;
  font-size: 22rpx;
  color: #2b3f7d;
  text-align: center;
  border: solid 2rpx #2b3f7d;
  position: absolute;
  right: -90rpx;
  top: 0;
}
.submit-success-box-two {
  margin-top: 40rpx;
}
.submit-success-box-two .two-title {
  font-size: 26rpx;
  text-align: center;
}
.submit-success-box-there {
  display: flex;
  justify-content: center;
  margin-top: 30rpx;
}
.submit-success-box-there .wxzf-image {
  width: 74%;
  display: flex;
}
.submit-success-box-there .wxzf-image image {
  width: 100%;
}

