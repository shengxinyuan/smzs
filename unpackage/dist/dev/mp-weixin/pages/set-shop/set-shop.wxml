<view class="zl-page"><view class="box-one"><view class="zl-heads"><view class="heads"><text>金店logo</text></view><view data-event-opts="{{[['tap',[['shopLogoPopup',['$event']]]]]}}" class="images" bindtap="__e"><image class="shop-photo" src="{{shopPhoto}}" mode="aspectFill"></image><u-icon class="icon xiangyou" vue-id="eedee3c0-1" name="arrow-right" bind:__l="__l"></u-icon></view></view><view class="zl-heads"><view class="heads"><text>金店名称</text></view><view data-event-opts="{{[['tap',[['skipShopName',['$event']]]]]}}" class="nick-name" bindtap="__e"><text>{{data.title}}</text><u-icon class="icon xiangyou" vue-id="eedee3c0-2" name="arrow-right" bind:__l="__l"></u-icon></view></view><view class="zl-heads"><view class="heads"><text>金店签名</text></view><view data-event-opts="{{[['tap',[['skipShopSignature',['$event']]]]]}}" class="nick-name" bindtap="__e"><text class="u-line-1">{{data.remark||'还没设置签名~'}}</text><u-icon class="icon xiangyou" vue-id="eedee3c0-3" name="arrow-right" bind:__l="__l"></u-icon></view></view><view class="zl-heads"><view class="heads"><text>客服电话</text></view><view data-event-opts="{{[['tap',[['skipServiceTel',['$event']]]]]}}" class="nick-name" bindtap="__e"><text>{{data.telephone||'还没设置手机号~'}}</text><u-icon class="icon xiangyou" vue-id="eedee3c0-4" name="arrow-right" bind:__l="__l"></u-icon></view></view><view class="zl-heads"><view class="heads"><text>金店地址</text></view><view data-event-opts="{{[['tap',[['skipShopAddress',['$event']]]]]}}" class="nick-name" bindtap="__e"><text>{{data.province+data.city+data.area+data.address}}</text><u-icon class="icon xiangyou" vue-id="eedee3c0-5" name="arrow-right" bind:__l="__l"></u-icon></view></view><view class="zl-heads"><view class="heads"><text>金店客服二维码</text></view><view data-event-opts="{{[['tap',[['skipServiceCode',['$event']]]]]}}" class="images" bindtap="__e"><image class="shop-code" src="{{data.qr_code}}" mode="aspectFill" data-event-opts="{{[['tap',[['imgpreview',['$0'],['data.qr_code']]]]]}}" bindta