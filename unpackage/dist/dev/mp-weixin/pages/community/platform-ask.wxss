.tabs-box {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-bottom: solid 2rpx #f2f2f2;
  padding-top: 60rpx;
  position: fixed;
  left: 0;
  top: 0;
  background-color: white;
  z-index: 20;
}
.tabs-box .icon-box {
  width: 15%;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
}
.tabs-box .icon-box .icon {
  font-size: 40rpx;
  color: #666666;
}
.tabs-box .zl_sty {
  width: 60%;
  margin-top: 20rpx;
  text-align: center;
  padding-left: 13%;
}
.yltk-details-list {
  padding: 180rpx 20rpx 20rpx 20rpx;
}
.yltk-details-list .yltk-details-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 20rpx 0;
}
.yltk-details-list .yltk-details-box .details-box-left {
  width: 74%;
  padding-right: 20rpx;
}
.yltk-details-list .yltk-details-box .details-box-left .details-text {
  font-size: 26rpx;
}
.yltk-details-list .yltk-details-box .details-box-left .bottom-show {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: end;
  -webkit-align-items: flex-end;
          align-items: flex-end;
  font-size: 24rpx;
  color: #999999;
  margin-top: 80rpx;
}
.yltk-details-list .yltk-details-box .details-box-right {
  width: 26%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.yltk-details-list .yltk-details-box .details-box-right image {
  width: 100%;
  height: 180rpx;
  border-radius: 6rpx;
}

