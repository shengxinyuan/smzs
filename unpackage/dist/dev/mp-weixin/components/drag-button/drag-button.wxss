.drag {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  color: #fff;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  font-size: 24rpx;
  position: fixed;
  z-index: 999;
}
.drag.transition {
  -webkit-transition: left .3s ease,top .3s ease;
  transition: left .3s ease,top .3s ease;
}

