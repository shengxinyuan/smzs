.header{width:100%;padding:0 30rpx;display:flex;justify-content:space-between;align-items:center;background-color:#fff}.header .input-view{width:calc(84% - 90px);display:flex;align-items:center;justify-content:space-between;padding:0 15px;background-color:#eee;border-radius:40px}.header .input-view input{height:60rpx;font-size:28rpx}.header .rig{display:flex;align-items:center}.content{background-color:#f8f8f8;display:flex}.left-aside{flex-shrink:0;width:200rpx;padding-bottom:140rpx;background-color:#fff}.f-item{display:flex;align-items:center;justify-content:center;width:100%;height:100rpx;position:relative}.f-item.active{font-weight:700;background:#f8f8f8;color:#2d407a}.f-item.active:before{content:"";position:absolute;left:20rpx;top:50%;-webkit-transform:translateY(-50%);transform:translateY(-50%);height:30rpx;width:10rpx;background-color:#2d407a;border-radius:6rpx 0rpx 0rpx 6rpx;opacity:.8}.right-aside{width:520rpx;flex:1;overflow:hidden;padding-left:20rpx;padding-bottom:140rpx}.right-aside .swiper{width:100%;height:100%}.right-aside .swiper image{width:100%;height:100%}.s-item{display:flex;align-items:center;height:70rpx;padding-top:8rpx;font-size:28rpx}.one_list_sty{background-color:#fff;margin-top:10rpx;padding-left:20rpx}.t-list{display:flex;flex-wrap:wrap;width:100%;background:#fff;padding-top:20rpx}.t-list:after{content:"";flex:99;height:0}.t-item{flex-shrink:0;display:flex;justify-content:center;align-items:center;flex-direction:column;width:32%;margin-right:2%;font-size:26rpx;color:#666;padding-bottom:20rpx}.t-item image{width:140rpx;height:140rpx;background-color:#f1f1f1}.t-item:nth-child(3n+3){margin-right:0}