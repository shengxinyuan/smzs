.zl-box{padding:20rpx 30rpx;border-top:solid 2rpx #eee}.zl-box .selection-box{width:100%;height:350rpx;margin-bottom:30rpx;display:flex;position:relative}.zl-box .selection-box image{width:100%;height:100%;border-radius:10rpx;background-color:#eee}.zl-box .selection-box .selection-title{width:100%;height:62rpx;padding:0 20rpx;background-image:linear-gradient(268deg,#094451,#02343f);border-radius:0 0 10rpx 10rpx;display:flex;align-items:center;font-size:26rpx;color:#fff;position:absolute;bottom:0;left:0}